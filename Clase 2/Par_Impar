dato1= int(input("Ingrese el primer numero: "))
dato2= int(input("Ingrese el segundo numero: "))


if dato1%2==0 and dato2%2==0:
    print("Ambos numeros son pares")
elif dato1%2==0 and dato2%2!=0:
    print(f"El numero {dato1} es par")
    print(f"El numero {dato2} es impar")
else:
    print(f"El numero {dato1} es impar")
    print(f"El numero {dato2} es par")


